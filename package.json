{
  "name": "wifi-stats-cli",
  "version": "0.1.0",
  "description": "macOS Wi-Fi diagnostics CLI",
  "type": "module",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "wifi-stats": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "wifi",
    "network",
    "macos",
    "diagnostics",
    "cli"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
